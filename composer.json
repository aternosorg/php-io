{
    "name": "aternos/io",
    "description": "PHP library for IO operations",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Aternos\\IO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aternos\\IO\\Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Matthias Neid",
